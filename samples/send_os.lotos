process os_send[class.send, class.sent, class.notsent] :=
    os.req ?msg;
    class.send !msg;
    (
        class.sent;
        os.complete;
        os_send[class.send, class.sent, class.notsent]
    []
        class.notsent ?err;
        os.failed !err;
        os_send[class.send, class.sent, class.notsent]
    )
endproc
